             xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
             x:Class="SAMS.StudorLecRegPopup"
           Size="300,300"
           IsLightDismissEnabled="False"
           HorizontalOptions="Center"
           VerticalOptions="Center"
           Color="Transparent"
           xct:CornerRadiusEffect.CornerRadius="30">
